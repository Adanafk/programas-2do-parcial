#include <stdio.h>
#include <iostream>

int main()
{
    system("color 3e");
  int i;
  printf("escribe un numero del 1 al 5 para mostrar\nla secuencia correspondiente\n");
  scanf("%d",&i);
if(i==5)
{
   for(i=i*i;i>=15;i=i-2)
 {
 printf("%d\t",i);
}
} else if(i==4)
{
    for(i=i*i;i>=8;i=i-2)
 {
 printf("%d\t",i);
}
}else if(i==3)
{
    for(i=i*i;i>=3;i=i-2)
 {
 printf("%d\t",i);
}
}else if(i==2)
{
    for(i=i*i;i>=0;i=i-2)
 {
 printf("%d\t",i);
}
}else if(i==1)
{
    for(i=i*i;i>=-1;i=i-2)
 {
 printf("%d\t",i);
}
}else
{
    if(i==0)
    {
        printf("%d\t",i);
    }
}
}
