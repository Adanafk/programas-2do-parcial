#include <stdio.h>
#include <iostream>

int main()
{
    system("color 4e");
  int i;
  printf("escribe un numero del 1 al 5 para mostrar\nla secuencia correspondiente\n");
  scanf("%d",&i);
if(i==5)
{
   for(i=i*3;i<=37;i=i+2)
 {
 printf("%d\t",i);
}
} else if(i==4)
{
    for(i=i*3;i<=30;i=i+2)
 {
 printf("%d\t",i);
}
}else if(i==3)
{
    for(i=i*3;i<=23;i=i+2)
 {
 printf("%d\t",i);
}
}else if(i==2)
{
    for(i=i*3;i<=16;i=i+2)
 {
 printf("%d\t",i);
}
}else if(i==1)
{
    for(i=i*3;i<=9;i=i+2)
 {
 printf("%d\t",i);
}
}else
{
    if(i==0)
    {
        for(i=i*3;i<=2;i=i+2)
        {
          printf("%d\t",i);
        }
    }
}
}
